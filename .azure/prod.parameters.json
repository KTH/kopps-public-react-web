{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "appName": {
      "value": "kopps-public-react-web-prod"
    },
    "secretNames": {
      "value": [
        "APPINSIGHTS_INSTRUMENTATIONKEY",
        "REDIS_URI",
        "SESSION_KEY",
        "SESSION_SECRET",
        "PDF_RENDER_FUNCTION_SUBSCRIPTION_KEY"
      ]
    },
    "environmentVariables": {
      "value": {
        "SERVER_HOST_URL": "https://www.kth.se",
        "SERVER_PORT": "3000",
        "SERVICE_PUBLISH": "/student/kurser",
        "KOPPS_API_BASEURL": "https://api.kth.se/api/kopps/v2/?defaultTimeout=60000",
        "CAS_SSO_URI": "https://login.kth.se",
        "CM_HOST_URL": "https://www.kth.se/cm/",
        "CONSOLE_LOG": "false",
        "NODE_ENV": "production",
        "PDF_RENDER_FUNCTION": "https://integral-api.sys.kth.se/PDFRenderFunction",
        "SESSION_USE_REDIS": "true",
        "CONSOLE_ENABLED": "false",
        "STDOUT_ENABLED": "true",
        "LOGGING_LEVEL": "INFO",
        "LOGGING_ACCESS_LOG": "true",
        "LOGGING_USER": "kopps-public-react-web"
      }
    },
    "monitorPath": {
      "value": "/student/kurser/_monitor"
    },
    "websitePort": {
      "value": "3000"
    }
  }
}
