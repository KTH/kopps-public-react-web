{
  "name": "kopps-public-react-web",
  "version": "2.0.0",
  "description": "Migration from Kopps in Play Framework 2.X to Express and React.",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/node-web"
  },
  "engines": {
    "node": "20"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "docker:pack-dev": "bash ./build.sh docker",
    "docker:build": "bash ./docker-build-image.sh dev",
    "docker:run": "bash ./docker-run-image.sh dev",
    "docker:start-dev": "npm run docker:build && npm run docker:run",
    "docker:test-dev:performance": "ID=$(docker build -f Dockerfile-dev -q .) && LOCAL_IMAGE_ID=$ID docker-compose --env-file .env -f docker-compose-performance-tests.yml up --build --abort-on-container-exit --remove-orphans",
    "docker:test-prod:performance": "ID=$(docker build -f Dockerfile-performance-test -q .) && LOCAL_IMAGE_ID=$ID docker-compose -f docker-compose-performance-tests.yml up --build --abort-on-container-exit --remove-orphans",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\" --quiet",
    "lint-v": "eslint \"{public,server}/**/*.{js,jsx}\" ",
    "prepare": "husky install",
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "start": "bash -c 'cat /KTH_NODEJS; NODE_ENV=production node app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "start-in-dev-docker": "bash -c 'NODE_ENV=development HAS_PRUNED_DEV_DEPEND=true node --inspect=0.0.0.0 app.js'",
    "start-in-prod-performance-docker": "bash -c 'NODE_ENV=production TEST_TYPE=performance node app.js'",
    "test": "cross-env NODE_ENV=test jest",
    "test:components": "NODE_ENV=test jest ./public/js/app/components/SearchResultDisplay.test.js --watch --notify",
    "test-dev:load": "echo 'Start your service before running this test' && docker-compose -f test/performance-artillery-dev/docker-compose.yml up --build --abort-on-container-exit --remove-orphans",
    "test-ref:load": "docker-compose -f test/performance-artillery-ref-stage/docker-compose.yml up --build --abort-on-container-exit --remove-orphans",
    "test:notify": "NODE_ENV=test jest --watch --notify",
    "test:pages": "NODE_ENV=test jest ./public/js/app/pages/CourseSearchLayout.test.js --watch --notify",
    "test:watch": "NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.24.0",
    "@kth/api-call": "^4.2.1",
    "@kth/appinsights": "^0.6.0",
    "@kth/cortina-block": "^7.0.0",
    "@kth/kth-node-web-common": "9.4.1",
    "@kth/kth-reactstrap": "^0.4.78",
    "@kth/log": "^4.0.7",
    "@kth/monitor": "^4.2.1",
    "@kth/om-kursen-ladok-client": "^2.4.0",
    "@kth/server": "^4.1.0",
    "@kth/session": "^3.0.9",
    "@kth/style": "^1.6.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "axios": "^1.11.0",
    "body-parser": "^1.20.2",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-handlebars": "^7.1.2",
    "kth-node-access-log": "^0.2.9",
    "kth-node-configuration": "^2.1.0",
    "kth-node-express-routing": "^2.2.0",
    "kth-node-i18n": "^1.0.18",
    "kth-node-redis": "^3.3.0",
    "kth-style": "^10.3.0",
    "mobx": "^6.12.0",
    "mobx-react": "^9.1.0",
    "prop-types": "^15.8.1",
    "querystring": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router": "^6.22.2",
    "react-router-dom": "^6.22.2",
    "reactstrap": "^9.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.28.3",
    "@babel/core": "^7.28.4",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@kth/eslint-config-kth": "^3.6.0",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^14.3.1",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.19.17",
    "@types/webpack": "^5.28.5",
    "@types/webpack-env": "^1.18.8",
    "babel-loader": "^9.2.1",
    "concurrently": "^8.2.2",
    "css-loader": "^6.11.0",
    "decache": "^4.6.2",
    "eslint": "^8.57.1",
    "file-loader": "^6.2.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "jest-axe": "^8.0.0",
    "jest-environment-jsdom": "30.1.2",
    "jest-extended": "^4.0.2",
    "lint-staged": "^15.5.2",
    "mini-css-extract-plugin": "^2.9.4",
    "node-mocks-http": "^1.17.2",
    "node-notifier": "^10.0.1",
    "nodemon": "^3.1.10",
    "null-loader": "^4.0.1",
    "path": "^0.12.7",
    "prettier": "^3.6.2",
    "sass": "^1.93.2",
    "sass-loader": "^13.3.3",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.14",
    "ts-loader": "^9.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^5.1.4"
  },
  "// (prettier)": "configuration version 2020-06-15 - might also reside in .prettierrc.json",
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 2
  },
  "lint-staged": {
    "**/*.{js,jsx}": [
      "npx prettier --write"
    ]
  }
}
