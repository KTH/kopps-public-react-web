{
  "name": "kopps-public-react-web",
  "version": "2.0.0",
  "description": "Migration from Kopps in Play Framework 2.X to Express and React.",
  "main": "app.js",
  "private": true,
  "license": "MIT",
  "author": "KTH",
  "repository": {
    "type": "git",
    "uri": "https://github.com/KTH/node-web"
  },
  "engines": {
    "node": "16"
  },
  "scripts": {
    "build": "bash ./build.sh prod",
    "build-dev": "bash ./build.sh dev",
    "lint": "eslint \"{public,server}/**/*.{js,jsx}\" --quiet",
    "lint-v": "eslint \"{public,server}/**/*.{js,jsx}\" ",
    "prepare": "husky install",
    "postinstall": "bash -c 'rm -rf ./dist; rm -rf ./node_modules/.cache/webpack'",
    "prettier:all": "prettier --write \"**/*.js\" \"**/*.jsx\" \"**/*.scss\"",
    "start": "bash -c 'NODE_ENV=production node --inspect=0.0.0.0 app.js'",
    "start-dev": "bash -c 'NODE_ENV=development concurrently --kill-others -n build,app \"npm run build-dev\" \"nodemon app.js\"'",
    "test": "cross-env NODE_ENV=test jest",
    "test:components": "NODE_ENV=test jest ./public/js/app/components/SearchResultDisplay.test.js --watch --notify",
    "test:notify": "NODE_ENV=test jest --watch --notify",
    "test:pages": "NODE_ENV=test jest ./public/js/app/pages/CourseSearchLayout.test.js --watch --notify",
    "test:watch": "NODE_ENV=test jest --watch"
  },
  "dependencies": {
    "@babel/runtime": "^7.17.2",
    "@kth/api-call": "^4.0.35",
    "@kth/kth-node-web-common": "^7.0.9",
    "@kth/kth-reactstrap": "^0.4.47",
    "@kth/log": "^4.0.1",
    "@kth/monitor": "^3.0.22",
    "@kth/server": "^4.0.1",
    "@kth/session": "^3.0.2",
    "applicationinsights": "^2.2.1",
    "axios": "^0.24.0",
    "body-parser": "^1.19.2",
    "chalk": "^5.0.0",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "express-handlebars": "^5.3.5",
    "kth-node-access-log": "^0.2.7",
    "kth-node-configuration": "^2.0.10",
    "kth-node-express-routing": "^2.0.12",
    "kth-node-i18n": "^1.0.12",
    "kth-node-redis": "^3.1.37",
    "kth-style": "7.0.12",
    "mobx": "^6.3.13",
    "mobx-react": "^7.2.1",
    "moment-timezone": "^0.5.34",
    "prop-types": "^15.8.1",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "reactstrap": "^8.10.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.3",
    "@babel/core": "^7.17.4",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@kth/eslint-config-kth": "^2.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@types/jest": "^27.4.0",
    "babel-loader": "^8.2.3",
    "concurrently": "^7.0.0",
    "css-loader": "^6.6.0",
    "eslint": "^7.32.0",
    "file-loader": "^6.2.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-axe": "^5.0.1",
    "jest-extended": "^1.2.1",
    "mini-css-extract-plugin": "^2.5.3",
    "nodemon": "^2.0.15",
    "null-loader": "^4.0.1",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "sass": "^1.49.7",
    "sass-loader": "^12.6.0",
    "set-value": "^4.1.0",
    "terser-webpack-plugin": "^5.3.1",
    "webpack": "^5.69.0",
    "webpack-cli": "^4.9.2"
  },
  "// (prettier)": "configuration version 2020-06-15 - might also reside in .prettierrc.json",
  "prettier": {
    "arrowParens": "avoid",
    "printWidth": 120,
    "semi": false,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "// (babel)": "configuration version 2020-10-21 (WEB) - might also reside in .babelrc.json",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  }
}
